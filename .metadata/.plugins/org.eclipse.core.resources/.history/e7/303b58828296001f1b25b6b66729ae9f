package com.first.project;

class Parent{
	public void show() {
		System.out.println("I am in Parent Class.");
	}
}

class Child extends Parent{

	@Override
	public void show() {
		System.out.println("I am in Child Class.");
	}
}
public class Annotation01 {

	public static void main(String[] args) {
		Child obj = new Child();
		obj.show();

	}

}
