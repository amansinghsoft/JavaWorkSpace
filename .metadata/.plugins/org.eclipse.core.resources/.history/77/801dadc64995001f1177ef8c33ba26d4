package com.first.project;


enum Laptop{
	Hp(5000), Dell(60000), Lenovo, Samsong(980000);  //Constructor
	
	private int price;
	
	private Laptop() {  //Default Constructor
		 price = 56000;
	}

	private Laptop(int price) {  //Parameterize Constructor
		this.price = price;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
}
public class Enum03 {

	public static void main(String[] args) {
		Laptop lap = Laptop.Dell;
		

	}

}
