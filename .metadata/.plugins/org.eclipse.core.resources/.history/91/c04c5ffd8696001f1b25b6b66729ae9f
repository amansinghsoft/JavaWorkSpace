package com.first.project;

@FunctionalInterface
interface A1 {
	void show();
}

//class B1 implements A1 {
//	public void show() {
//		System.out.println("I am in Show");
//	}
//}

public class FunctionalInterface {

	public static void main(String[] args) {
		A1 obj = new A1() {
			public void show() {    //Anonymous Inner Class
				System.out.println("I am in Show");
			}
		};

		obj.show();

	}

}
